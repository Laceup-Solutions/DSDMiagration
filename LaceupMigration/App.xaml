<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:LaceupMigration"
             xmlns:converters="clr-namespace:LaceupMigration.Converters"
             x:Class="LaceupMigration.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            
            <converters:IsStringNotEmptyConverter x:Key="IsStringNotEmptyConverter" />
            <converters:IsNotZeroConverter x:Key="IsNotZeroConverter" />
            <converters:InvertedBoolConverter x:Key="InvertedBoolConverter" />
            <converters:CreditTypeConverter x:Key="CreditTypeConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
